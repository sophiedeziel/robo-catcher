<% sub_sequence_instance = Sequence.find(sub_sequence.sequence_id) %>

<div data-toggle="collapse"  data-target="#sub-instructions-<%= sub_sequence.id %>" aria-expanded="false" aria-controls="sub-instructions-<%= sub_sequence.id %>" >
  <div class="name">
    <%= sub_sequence.human_name %> <%= sub_sequence_instance.name %>
  </div>
</div>
<div class="collapse multi-collapse offset-md-1 sub-instructions" id="sub-instructions-<%= sub_sequence.id %>">
  <% sub_sequence_instance.instructions.each do |instruction| %>
    <div class="instruction" id="instruction-<%= instruction.id %>">
      <%= render instruction %>
    </div>
  <% end %>
</div>